{
  "expo": {
    "name": "Presby Companion",
    "description": "Presbyterian Church of Ghana mobile app",
    "slug": "presby-companion",
    "privacy": "public",
    "sdkVersion": "36.0.0",
    "platforms": [
      "ios",
      "android"
    ],
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icons/icon.png",
    "splash": {
      "image": "./assets/icons/icon.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "assets/images/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.polymorphlabs.presbycompanion"
    },
    "android": {
      "package": "io.polymorphlabs.presbycompanion"
    }
  }
}
