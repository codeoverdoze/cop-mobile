{
  "expo": {
    "name": "Presby Companion",
    "description": "Mobile app companion for the Presbyterian Church of Ghana",
    "slug": "presby-companion",
    "privacy": "unlisted",
    "sdkVersion": "36.0.0",
    "platforms": ["ios", "android"],
    "version": "1.0.2",
    "orientation": "portrait",
    "icon": "./assets/icons/android_icon.png",
    "splash": {
      "image": "./assets/images/splash.png",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 30
    },
    "assetBundlePatterns": ["assets/images/*", "assets/icons/*"],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.polymorphlabs.presbycompanion",
      "icon": "./assets/icons/ios_icon.png"
    },
    "androidStatusBar": {
      "backgroundColor": "#387ecb"
    },
    "android": {
      "package": "com.polymorphlabs.presbycompanion"
    }
  }
}
