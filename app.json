{
  "expo": {
    "name": "pcg-mobile-app",
    "description": "Presbyterian Church of Ghana mobile app",
    "slug": "pcg-mobile-app",
    "privacy": "public",
    "sdkVersion": "36.0.0",
    "platforms": [
      "ios",
      "android"
    ],
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "splash": {
      "image": "./assets/images/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "assets/images/*"
    ],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "package": "com.polymorph.pcg"
    }
  }
}
